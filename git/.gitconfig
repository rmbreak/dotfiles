[user]
	email = mack@mackstump.com
	name = Mack Stump
[alias]
    s  = status
	lg = log --pretty=oneline --abbrev-commit --graph --all --decorate
[color]
	ui = true
[core]
	editor = nvim
	pager = diff-so-fancy | less --tabs=4 -RFX
[push]
	default = simple
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
